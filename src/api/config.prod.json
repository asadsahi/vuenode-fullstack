{
  "appTitle": "Vue NodeJs fullstack",
  "ssrEnabled": true,
  "database": {
    "dialect": "sqlite",
    "storage": "./vuenode-fullstack.sqlite"
  },
  "Security": {
    "JWT_SECRET": "SECRET",
    "SESSION_SECRET": "SECRET"
  },
  "profile": {
    "fileSizeLimit": "1048576"
  },
  "whitelistedUserFields": [
    "firstName",
    "lastName",
    "email",
    "username",
    "displayName",
    "isAdmin",
    "roles",
    "profileImage"
  ],
  "illegalUsernames": [
    "administrator",
    "password",
    "admin",
    "user",
    "unknown",
    "anonymous",
    "null",
    "undefined",
    "api"
  ],
  "facebook": {
    "clientID": "changeMe",
    "clientSecret": "changeMe",
    "callbackURL": "/api/auth/facebook/callback"
  },
  "google": {
    "clientID": "changeMe",
    "clientSecret": "changeMe",
    "callbackURL": "/api/auth/google/callback"
  },
  "github": {
    "clientID": "changeMe",
    "clientSecret": "changeMe",
    "callbackURL": "/api/auth/github/callback"
  },
  "mailOptions": {
    "from": "'Vue Node Fullstack' <admin@vuenodefullstack.com>",
    "service": "gmail",
    "auth": {
      "user": "email",
      "pass": "password"
    }
  }
}
